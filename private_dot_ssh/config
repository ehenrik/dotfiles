Include config.d/*

Host *.k8s.unbound.se
  IdentityFile /Users/argoyle/.ssh/id_rsa
  User admin

Host gitlab.com
  User git

Host homeassistant.local
  User root
  Port 2222

Host *
  User ubuntu
  StrictHostKeyChecking no
  ServerAliveInterval 30
  ForwardAgent yes
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa

